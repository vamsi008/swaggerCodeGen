<!DOCTYPE web-app PUBLIC
		"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
		"http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Web Application to develop Swagger.</display-name>

	<servlet>
		<servlet-name>com.employee.app.EmployeeServiceApp</servlet-name>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>com.employee.app.EmployeeServiceApp</servlet-name>
		<url-pattern>/sample/*</url-pattern>
	</servlet-mapping>
	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>
			classpath:rest_application_context.xml
		</param-value>
	</context-param>
</web-app>
